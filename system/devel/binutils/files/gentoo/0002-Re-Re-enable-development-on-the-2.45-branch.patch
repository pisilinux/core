   case $ac_init_help in
-     short | recursive ) echo "Configuration of bfd 2.45:";;
+     short | recursive ) echo "Configuration of bfd 2.45.0:";;
    esac
   cat <<\_ACEOF
 
@@ -1625,7 +1625,7 @@ fi
