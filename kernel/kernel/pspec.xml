        <AdditionalFiles>
            <AdditionalFile target="configs/kernel-x86_64-config">configs/kernel-x86_64-config</AdditionalFile>
            <AdditionalFile permission="0755" target="generate-module-list">scripts/generate-module-list</AdditionalFile>
        </AdditionalFiles>
